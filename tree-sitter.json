{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "harmony",
      "camelcase": "Harmony",
      "title": "Harmony",
      "scope": "source.harmony",
      "file-types": [
        "harmony"
      ],
      "injection-regex": "^harmony$",
      "class-name": "TreeSitterHarmony"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "OpenAI's Harmony Response Format (gptoss)",
    "authors": [
      {
        "name": "Wes Higbee"
      }
    ],
    "links": {
      "repository": "https://github.com/g0t4/tree-sitter-harmony"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}